@article{Cameirao2010,
abstract = {Age of acquisition (AoA) is an important psycholinguistic variable that affects the speed and accuracy of lexical processing in tasks such as word naming, picture naming, and lexical decision. In the present work, we collected AoA ratings for 1,749 Portuguese words (nouns, verbs, adjectives, and adverbs), using a 9-point scale that was first proposed by Carroll and White (1973). We analyzed the relation between AoA ratings and other psycholinguistic variables (length measures, neighborhood density, written-word frequency, familiarity, imageability, and concreteness), and we assessed reliability by correlating our ratings with those from other databases presented for Portuguese, English, Spanish, and Italian. The full database can be downloaded from http://brm.psychonomic-journals.org/content/supplemental.},
