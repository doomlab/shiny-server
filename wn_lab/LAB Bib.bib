}
@article{Vincent2006,
abstract = {Anagram tasks are frequently used in cognitive research, and the generation of new scrambled letter combinations is a task well suited to a software solution. Most available programs, however, do not allow experimenters to generate new anagrams flexibly or to characterize existing anagrams using psycholinguistic criteria. They also do not provide detailed information on their source dictionaries. We present anagram software that interfaces with CELEX2, an internationallyrecognized psycholinguistic database. This software allows users to capitalize on lexical variables and thus enables direct control of psycholinguistic features that may influence the cognitive processes involved in anagram solution.},
