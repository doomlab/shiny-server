journal = {Annales des Telecommunications/Annals of Telecommunications},
keywords = {Linguistic processing,Living/nonliving domains,Normative data,Semantic categories,Semantic verbal fluency},
number = {4},
pages = {1088--1097},
pmid = {24415408},
title = {{Spanish norms for age of acquisition, concept familiarity, lexical frequency, manipulability, typicality, and other variables for 820 words from 14 living/nonliving concepts}},
volume = {46},
year = {2014}
}
