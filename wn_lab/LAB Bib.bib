journal = {Behavior Research Methods},
keywords = {Actions,Culture,Familiarity},
number = {4},
pages = {946--953},
pmid = {22359253},
title = {{An action video clip database rated for familiarity in China and Germany}},
volume = {44},
year = {2012}
}
