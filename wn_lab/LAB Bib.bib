pages = {510--526},
pmid = {17958162},
title = {{Extracting semantic representations from word co-occurrence statistics: A computational study}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17958162 http://www.springerlink.com/index/10.3758/BF03193020},
volume = {39},
